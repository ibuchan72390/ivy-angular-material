  },
  "license": "ISC",
  "scripts": {
    "test": "karma start ./karma.conf.js",
    "test:chrome": "karma start --browsers Chrome ./karma.conf.js",
    "test:firefox": "karma start --browsers Firefox ./karma.conf.js",
    "test:all": "karma start --browsers Chrome,Firefox,PhantomJS ./karma.conf.js",
    "test-debug": "karma start --no-single-run ./karma.conf.js",
    "test-debug:chrome": "karma start --no-single-run --browsers Chrome ./karma.conf.js",
    "test-debug:firefox": "karma start --no-single-run --browsers Firefox ./karma.conf.js",
    "prepublish": "tsc",
    "test-publish": "tsc && npm link"
  },
